{
  "name": "facerace",
  "version": "0.0.1",
  "description": "3D multiplayer racing in your browser",
  "author": "Blake La Pierre",
  "scripts": {
    "postinstall": "grunt serve"
  },
  "dependencies": {
    "express": "~3.5.1",
    "socket.io": "~0.9.16",
    "webrtc.io": "0.0.4",
    "webrtc.io-client": "0.0.2-3",
    "ottypes": "git+https://github.com/blakelapierre/ottypes",
    "lodash": "~2.4.1"
  },
  "devDependencies": {
    "grunt": "~0.4.4",
    "grunt-bg-shell": "~2.3.1",
    "browserify": "3.33.0",
    "browserify-shim": "3.3.2",
    "partialify": "~3.1.1",
    "uglifyify": "~2.1.1",
    "grunt-cli": "~0.1.13"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "partialify"
    ]
  },
  "browserify-shim": {
    "angular": "global:angular",
    "three": "global:THREE",
    "stats": "global:Stats",
    "webrtc.io": "global:rtc",
    "socket.io": "global:io",
    "underscore": "global:_",
    "lodash": "global:_",
    "mathjs": "global:mathjs"
  }
}
