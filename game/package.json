{
  "name": "facerace",
  "version": "0.0.1",
  "description": "3D multiplayer racing in your browser",
  "author": "Blake La Pierre",
  "dependencies": {
    "express": "latest",
    "socket.io": "latest",
    "webrtc.io": "latest",
    "webrtc.io-client": "latest"
  },
  "devDependencies": {
    "grunt": "latest",
    "grunt-bg-shell": "latest",
    "browserify": "3.33.0",
    "browserify-shim": "3.2.0",
    "partialify": "latest"
  },
  "browserify": {
  	"transform": [
  		"browserify-shim",
  		"partialify"
  	]
  },
  "browserify-shim": {
  	"angular": "global:angular",
  	"three": "global:THREE",
    "stats": "global:Stats",
    "webrtc.io": "global:rtc",
    "socket.io": "global:io",
    "underscore": "global:_"
  }
}